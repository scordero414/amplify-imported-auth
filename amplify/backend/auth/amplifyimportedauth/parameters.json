{
  "authSelections": "userPoolOnly",
  "resourceName": "amplifyimportedauth",
  "serviceType": "imported",
  "region": "us-west-2",
  "usernameAttributes": [
    "email",
    "phone_number"
  ],
  "requiredAttributes": [
    "phone_number"
  ],
  "passwordPolicyMinLength": 8,
  "passwordPolicyCharacters": [
    "Requires Lowercase",
    "Requires Uppercase",
    "Requires Numbers",
    "Requires Symbols"
  ],
  "mfaConfiguration": "ON",
  "autoVerifiedAttributes": [
    "email",
    "phone_number"
  ],
  "mfaTypes": [
    "SMS Text Message"
  ]
}